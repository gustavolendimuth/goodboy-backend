    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-plugin-editorconfig": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-node": "^10.9.1",
