{
  "name": "goodboy-api",
  "version": "1.0.0",
  "description": "API for Mercado Pago checkout",
  "main": "build/index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon ./src/index.ts",
    "build": "npx -y tsc",
    "db:reset": "tsc && npm run db:migrate",
    "db:migrate": "sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
